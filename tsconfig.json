{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "lib": ["es5", "es6", "dom", "es7"],
    "typeRoots": ["./node_modules/@types"],
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["dist", "node_modules"]
}
